<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:themes="clr-namespace:AnakinRaW.AppUpdaterFramework.Themes">

    <SolidColorBrush x:Key="{x:Static themes:UpdateColors.UpdateProductBackground}" Color="#FF2E2E2E"/>
    <SolidColorBrush x:Key="{x:Static themes:UpdateColors.UpdateProductBorder}" Color="#FF707070"/>
    <SolidColorBrush x:Key="{x:Static themes:UpdateColors.UpdateInfoBarBackground}" Color="#FF181818"/>
    <SolidColorBrush x:Key="{x:Static themes:UpdateColors.UpdateInfoBarBorder}" Color="#FF181818"/>

</ResourceDictionary>