<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dialogViewModels="clr-namespace:AnakinRaW.ApplicationBase.ViewModels.Dialogs"
                    xmlns:dialogs="clr-namespace:AnakinRaW.ApplicationBase.Views.Dialogs"
                    xmlns:viewModels="clr-namespace:AnakinRaW.ApplicationBase.ViewModels"
                    xmlns:views="clr-namespace:AnakinRaW.ApplicationBase.Views">
    
   <DataTemplate DataType="{x:Type dialogViewModels:ErrorMessageDialogViewModel}">
        <dialogs:ErrorDialog/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:ApplicationVersionViewModel}">
        <views:ApplicationVersionPanel/>
    </DataTemplate>

    <!-- Update Dialogs -->

    <DataTemplate DataType="{x:Type dialogViewModels:UpdateErrorDialog}">
        <dialogs:ErrorDialog/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogViewModels:UpdateRestoreErrorDialog}">
        <dialogs:ErrorDialog/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogViewModels:KillProcessDialogViewModel}">
        <dialogs:KillProcessesDialog/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogViewModels:UpdateRestartDialog}">
        <dialogs:RestartDialog/>
    </DataTemplate>

</ResourceDictionary>