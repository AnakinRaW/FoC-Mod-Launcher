<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:FocLauncher.ViewModels"
                    xmlns:views="clr-namespace:FocLauncher.Views"
                    xmlns:dialogs="clr-namespace:FocLauncher.Views.Dialogs"
                    xmlns:dialogViewModels="clr-namespace:AnakinRaW.ApplicationBase.ViewModels.Dialogs;assembly=AnakinRaW.ApplicationBase">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/AnakinRaW.AppUpdaterFramework.WPF;component/DataTemplates.xaml"/>
        <ResourceDictionary Source="/AnakinRaW.ApplicationBase;component/DataTemplates.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <DataTemplate DataType="{x:Type viewModels:MainPageViewModel}">
        <views:MainPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogViewModels:ApplicationAboutDialogViewModel}">
        <dialogs:AboutDialog/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type views:Item}">
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="{Binding Text}"/>
        </StackPanel>
    </DataTemplate>
    
</ResourceDictionary>