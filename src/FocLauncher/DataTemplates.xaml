<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:FocLauncher.ViewModels"
                    xmlns:views="clr-namespace:FocLauncher.Views"
                    xmlns:dialogs="clr-namespace:FocLauncher.Dialogs"
                    xmlns:updateViewModels="clr-namespace:FocLauncher.Update.ViewModels"
                    xmlns:views1="clr-namespace:FocLauncher.Update.Views">
    
    <DataTemplate DataType="{x:Type viewModels:MainPageViewModel}">
        <views:MainPage/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModels:ErrorMessageDialogViewModel}">
        <dialogs:ErrorDialog/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:LauncherAboutDialogViewModel}">
        <dialogs:AboutDialog/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type views:Item}">
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="{Binding Text}"/>
        </StackPanel>
    </DataTemplate>


    <DataTemplate DataType="{x:Type updateViewModels:UpdateWindowViewModel}">
        <views1:UpdateWindow/>
    </DataTemplate>


    <DataTemplate DataType="{x:Type viewModels:LauncherVersionViewModel}">
        <views:LauncherVersionPanel/>
    </DataTemplate>

</ResourceDictionary>