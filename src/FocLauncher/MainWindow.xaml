<controls:ApplicationMainWindow x:Class="FocLauncher.MainWindow"
                           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                           xmlns:focLauncher="clr-namespace:FocLauncher"
                           xmlns:designer="clr-namespace:FocLauncher.ViewModels.Designer"
                           xmlns:theming="clr-namespace:Sklavenwalker.CommonUtilities.Wpf.Theming;assembly=Sklavenwalker.CommonUtilities.WPF.ThemedControls"
                           xmlns:controls="clr-namespace:Sklavenwalker.CommonUtilities.Wpf.ApplicationFramework.Controls;assembly=Sklavenwalker.CommonUtilities.WPF.ApplicationFramework"
                           mc:Ignorable="d"
                           Width="400" 
                           Height="700" 
                           Title="{x:Static focLauncher:LauncherEnvironment.LauncherProgramName}" 
                           d:DataContext="{d:DesignInstance {x:Type designer:ApplicationViewModel}, IsDesignTimeCreatable=True}"
                           theming:ScrollBarThemingUtilities.ThemeScrollBars="True">
    <ContentControl Focusable="False" Content="{Binding CurrentViewModel}" Margin="6"/>
</controls:ApplicationMainWindow>
